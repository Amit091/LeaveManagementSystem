<div class="table table-responsive-sm text-center">
    <table class="table  table-sm">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Type</th>
                <th scope="col">Date</th>
                <th>Options</th>
            </tr>
        </thead>
        <tbody>
            <% allresult.forEach(function(holiday){%>
            <tr>
                <td><%= holiday.name%></td>
                <td><%= holiday.type%></td>
                <td><%= holiday.dateformat%></td>
                <td>
                    <div class="btn-group btn-group-sm mr-0">
                        <a href="/holiday/editholiday/<%=holiday.id%>" class="btn btn-warning btn-sm">Edit&nbsp;<i
                                class="fa fa-pen"></i></a>
                        <a href="/holiday/deleteholiday/<%=holiday.id%>" class="btn btn-danger btn-sm">Delete&nbsp;<i
                                class="fa fa-times"></i></a>
                    </div>
                </td>
            </tr>
            <%})%>
        </tbody>
    </table>
</div>